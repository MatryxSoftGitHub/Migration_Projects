				sATObjClassName = lsATClassNames
				break
			case "@title="
				sATObjClassName = lsATClassNames
				break
			case "[text[]="
				sATObjClassName = lsATClassNames
				break
			case "[text[] ="
				sATObjClassName = lsATClassNames
				break
			case "[@type="
				sATObjClassName = lsATClassNames
				break
